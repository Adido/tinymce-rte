{
    "name": "TinyMCE Rich Text Editor",
    "lowCaseName": "tinymcerte",
    "description": "TinyMCE 4",
    "author": "John Peca",
    "version": "0.0.1",
    "package":{
        "elements": {
            "plugins": [{
                "name": "TinyMCERTE",
                "events": ["OnRichTextBrowserInit", "OnRichTextEditorRegister", "OnRichTextEditorInit"]
            }]
        },
        "systemSettings": [{
            "key": "plugins",
            "value": "advlist autolink lists link image charmap print preview anchor visualblocks searchreplace code fullscreen insertdatetime media table contextmenu paste"
        },{
            "key": "toolbar1",
            "value": "undo redo | styleselect | bold italic | alignleft aligncenter alignright alignjustify | bullist numlist outdent indent | link image"
        },{
            "key": "toolbar2",
            "value": ""
        },{
            "key": "toolbar3",
            "value": ""
        },{
            "key": "menubar",
            "value": "file edit insert view format table tools"
        },{
            "key": "statusbar",
            "value": true,
            "type": "combo-boolean"
        }]
    }
}